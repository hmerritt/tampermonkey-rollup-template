{
	"singleQuote": false,
	"tabWidth": 4,
	"semi": true,
	"printWidth": 90,
	"trailingComma": "none",
	"endOfLine": "lf",
	"plugins": [
		"@trivago/prettier-plugin-sort-imports"
	],
	"minKeys": 2,
	"importOrder": [
		"<THIRD_PARTY_MODULES>",
		"^lib/(.*)$",
		"^types/(.*)$",
		"^[./](.*)$"
	],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"importOrderGroupNamespaceSpecifiers": true
}
